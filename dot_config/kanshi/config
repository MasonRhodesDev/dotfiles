# Kanshi profiles that can be manually switched via kanshictl
profile lid_closed {
    output "Dell Inc. DELL S3422DWG HSRTS63" enable
    output "Dell Inc. DELL S2721QS 6VSGM43" enable
    output eDP-2 disable
}

profile lid_open {
    output "Dell Inc. DELL S3422DWG HSRTS63" enable
    output "Dell Inc. DELL S2721QS 6VSGM43" enable
    output eDP-2 enable
}